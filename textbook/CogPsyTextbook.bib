@article{miltonBehavioralNeuralSignatures2021,
  title = {Behavioral and {{Neural Signatures}} of {{Visual Imagery Vividness Extremes}}: {{Aphantasia}} versus {{Hyperphantasia}}},
  shorttitle = {Behavioral and {{Neural Signatures}} of {{Visual Imagery Vividness Extremes}}},
  author = {Milton, Fraser and Fulford, Jon and Dance, Carla and Gaddum, James and Heuerman-Williamson, Brittany and Jones, Kealan and Knight, Kathryn F and MacKisack, Matthew and Winlove, Crawford and Zeman, Adam},
